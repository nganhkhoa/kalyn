(import "Stdlib.kalyn")

(defn showInt (Func Int String)
  (i)
  (if (== i 0)
    "0"
    (let ((showInt*
           (lambda (i str)
             (if (== i 0)
               str
               (showInt*
                 (/ i 10)
                 (Cons
                   (Char (+ 0x30 (% i 10)))
                   str))))))
      (showInt* i ""))))

(public def main (IO Empty)
  (print (showInt 13)))
